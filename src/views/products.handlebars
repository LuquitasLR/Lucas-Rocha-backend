<div>
  <div>
     <h1>Bienvenido {{user}}!</h1>
     {{#if user}}
    <a href="/sessions/logout"><button>Logout!</button></a>
  {{/if}}
  </div>
   <h2>Lista de productos disponibles:</h2>
 <ol>{{#each paginatedProducts}}
    <li>
        <h3>Producto: {{this.title}}</h3>
        <h4>ID:</h4> <p>{{this._id}}</p>
        <h4>Descripción:</h4><p>{{this.description}}</p>
        <h3>Precio: ${{this.price}}</h3>
        <a href="javascript:addProduct({{cart.id}},{{this._id}})"><button>Agregar al carrito</button></a>
    </li>
    <hr/>
    {{/each}}
 </ol>
 <h3>paginacion</h3>
<p>
  {{#if hasPrevPage}}
    <a style='text-decoration: none;' href='/products?querypage={{prevPage}}'>⬅️{{prevPage}}  - - - -  </a>
  {{/if}}
  pagina
  {{page}}
  de
  {{totalPages}}
  {{#if hasNextPage}}
    <a style='text-decoration: none;' href='/products?querypage={{nextPage}}'>  - - - -  {{nextPage}}➡️</a>
  {{/if}}
</p>
</div>
<script src="src/services/cartService.js" ></script>